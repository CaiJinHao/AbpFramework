<Project Sdk="Microsoft.NET.Sdk">

  <Import Project="..\..\common.props" />

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>Jh.Abp.MenuManagement</RootNamespace>
    <GenerateEmbeddedFilesManifest>true</GenerateEmbeddedFilesManifest>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>KimHo</Authors>
    <Company>KimHo</Company>
    <Copyright>KimHo</Copyright>
    <PackageProjectUrl>https://github.com/CaiJinHao/Menu/src/Jh.Abp.MenuManagement.Domain.Shared</PackageProjectUrl>
    <RepositoryUrl>https://github.com/CaiJinHao/Menu/src/Jh.Abp.MenuManagement.Domain.Shared</RepositoryUrl>
    <PackageIcon>nugeticon.png</PackageIcon>
    <PackageTags>Abp</PackageTags>
    <PackageReleaseNotes>Jh.Abp.MenuManagement</PackageReleaseNotes>
    <Version>0.0.6</Version>
    <Description>Jh.Abp.MenuManagement</Description>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\..\..\document\images\nugeticon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Volo.Abp.Validation" Version="4.0.0" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.FileProviders.Embedded" Version="5.0.0" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Localization\MenuManagement\*.json" />
    <Content Remove="Localization\MenuManagement\*.json" />
  </ItemGroup>

</Project>
